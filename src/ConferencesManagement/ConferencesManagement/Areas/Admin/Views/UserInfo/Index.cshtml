@model List<Models.Framework.ThongTinUser>

    @{
        ViewBag.Title = "Index";
        Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
    }

    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>IDThongTin</th>
                            <th>IDAccount</th>
                            <th>Name</th>
                            <th>NgaySinh</th>
                            <th>SDT</th>
                            <th>Email</th>
                            <th>DiaChi</th>
                        </tr>
                    </thead>
                    @*<tfoot>
                        <tr>
                            <th>Name</th>
                            <th>Position</th>
                            <th>Office</th>
                            <th>Age</th>
                            <th>Start date</th>
                            <th>Salary</th>
                        </tr>
                    </tfoot>*@
                    <tbody>
                        @foreach (var item in Model)
                        {
                        <tr>
                            <td>@item.IdThongTin</td>
                            <td>@item.IdAccount</td>
                            <td>@item.HoTen</td>
                            <td>@item.NgaySinh.ToString("dd/mm/yyyy")</td>
                            <td>@item.SDT</td>
                            <td>@item.Email</td>
                            <td>@item.DiaChi</td>
                        </tr>
                        }


                    </tbody>
                </table>
            </div>
        </div>
    </div>

    </div>
    <!-- /.container-fluid -->
    </div>
    <!-- End of Main Content -->
